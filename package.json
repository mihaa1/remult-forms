{
	"name": "remult-uikit",
	"version": "0.0.62",
	"type": "module",
	"scripts": {
		"dev:playground": "vite",
		"dev": "tsx watch index.ts",
		"dev:node": "tsx watch playground/server",
		"build:vite": "tsc && vite build",
		"build": "rm -rf dist && tsc",
		"build:n:copy": "yarn build && yarn copy:files",
		"build:watch": "tsc --watch",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"publishme": "rm -rf dist && yarn build:n:copy && cd dist/src && yarn publish && cd ../.. && yarn version --patch && git push && git push --tags",
		"copy:files": "cp package.json dist && cp README.md dist",
		"prepare": "relative-deps"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:mihaa1/remult-uikit.git"
	},
	"license": "MIT",
	"author": "spiderman",
	"devDependencies": {
		"@emotion/react": "^11.11.1",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.15.1",
		"@mui/material": "^5.15.0",
		"@mui/system": "^5.15.0",
		"@mui/x-data-grid": "^6.18.5",
		"@mui/x-data-grid-generator": "^6.18.5",
		"@mui/x-date-pickers": "^6.18.4",
		"@types/express": "^4.17.21",
		"@types/react": "^18.2.43",
		"@types/react-dom": "^18.2.17",
		"@typescript-eslint/eslint-plugin": "^6.14.0",
		"@typescript-eslint/parser": "^6.14.0",
		"@vitejs/plugin-react": "^4.2.1",
		"dayjs": "^1.11.10",
		"eslint": "^8.55.0",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"express": "^4.18.2",
		"material-react-table": "^2.4.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "^6.21.0",
		"relative-deps": "^1.0.7",
		"remult": "^0.24.2-exp.0",
		"tsx": "^4.6.2",
		"typescript": "^5.2.2",
		"vite": "^5.0.8"
	},
	"relativeDependencies": {},
	"module": "./index.js",
	"types": "./index.d.ts"
}
